<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<!-- this config file is used to register third party libraries that support JSF -->  
	<!-- please see navigation.xml and spring-beans.xml for navigation and beans registration -->  
	<application>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
		
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
		
		<resource-bundle>
			<base-name>com.kanchutech.eden.common.label</base-name>
			<var>label</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>com.kanchutech.eden.common.message</base-name>
			<var>msg</var>
		</resource-bundle>
	
	</application>
</faces-config>
